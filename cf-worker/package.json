{
  "name": "linggen-analytics",
  "version": "1.0.0",
  "description": "Cloudflare Worker for Linggen usage analytics",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create linggen_analytics",
    "db:migrate": "wrangler d1 migrations apply linggen_analytics --local",
    "db:migrate:prod": "wrangler d1 migrations apply linggen_analytics --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.3.3",
    "wrangler": "^4.51.0"
  }
}
